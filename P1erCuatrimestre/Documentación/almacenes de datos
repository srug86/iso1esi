USUARIOS = @id_usuario + tipo + e-mail + contraseña + nombre + apellidos + teléfono + área + institución + currículum + palabras_clave

PAQUETES_DE_PROYECTOS = @id_paquete + institución + bases + convocatoria + fecha_entrada + fecha_salida + fecha_límite

MODELOS_DE_EVALUACIÓN = @id_modelo + estructura

ÁREAS = @id_área + nombre.

SUBÁREAS = @id_subárea + @id_área + id_usuario + nombre

PROYECTOS = @id_proyecto + @id_paquete + memoria + área + subárea + estado + tpo_inv + tpo_eval + id_modelo_eval + evaluación

EXPERTOS/PROYECTOS = @id_experto + @id_proyecto + id_informe + valoración + fecha_asignación + estado

INFORMES DE EVALUACIÓN = @id_informe + id_modelo_eval + estado + datos

PLANTILLA CORREO = @id_plantilla + asunto + cuerpo

