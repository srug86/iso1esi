PROCESO Añadir_Usuarios (usuarios)
  SI usuario_activo.tipo = "Secretario" ENTONCES
    AÑADIR usuarios A USUARIOS
    correo_alta <= (LEER PLANTILLAS_CORREOS.asunto = "Alta")
            + (usuarios.id_nombre) + (usuarios.correo)
            + (usuarios.contraseña)
    RETORNA correo_alta
  FIN SI
