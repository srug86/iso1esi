\subsubsection{Almacenes de datos}
﻿\textbf{USUARIOS} = @id\_usuario + tipo + e-mail + contraseña + nombre + apellidos + teléfono + id\_área + institución + currículum + palabras\_clave

\textbf{PAQUETES PROYECTOS} = @id\_paquete + nombre\_paquete + institución + bases + convocatoria + fecha\_entrada + fecha\_salida + fecha\_límite

\textbf{fecha\_entrada} = fecha\_salida = fecha\_límite = día + mes + año

\textbf{MODELOS EVALUACIÓN} = @id\_modelo + cppid + estructura

\textbf{cppid} = *\emph{Identificador de la convocatoria del paquete de proyectos correspondiente}*

\textbf{ÁREAS} = @id\_área + nombre

\textbf{SUBÁREAS} = @id\_subárea + @id\_área + id\_usuario + nombre

\textbf{SUBÁREAS} = *\emph{El id\_usuario es sólo para los adjuntos. El resto de usuarios no tiene área asignada}*

\textbf{PROYECTOS} = @id\_proyecto + @id\_paquete + nombre\_proyecto + memoria + id\_área + id\_subárea + estado\_proyecto + tpo\_inv + tpo\_eval + id\_modelo\_eval + evaluación

\textbf{tpo\_inv} = *\emph{Tiempo que tiene un experto para aceptar o declinar una invitación}*

\textbf{tpo\_eval} = *\emph{Tiempo que tiene un experto para realizar una evaluación desde su aceptación}*

\textbf{evaluación} = *\emph{Informe final realizado}*

\textbf{EXPERTOS-PROYECTOS} = @id\_usuario + @id\_paqproy + @id\_proyecto + id\_informe + valoración + fecha\_asignación

\textbf{valoración} = tpo\_realización + calidad\_informe

\textbf{fecha\_asignación} = día + mes + año

\textbf{tpo\_realización} = nota

\textbf{calidad\_informe} = nota

\textbf{INFORMES EVALUACIÓN} = @id\_informe + id\_modelo\_eval + estado\_informe + datos

\textbf{datos} = *\emph{El experto, el adjunto y el coordinador rellenarán los campos correspondientes al modelo de evaluación del proyecto}*

\textbf{PLANTILLAS CORREOS} = @id\_plantilla + asunto + cuerpo

\subsubsection{Datos Elementales}

\textbf{tipo} = [secretario \mid coordinador \mid adjunto \mid experto]

\textbf{dígito} = [0 \mid 1 \mid 2 \mid 3 \mid 4 \mid 5 \mid 6 \mid 7 \mid 8 \mid 9]

\textbf{día} = [1 \mid 2 \mid 3 \mid 4 \mid 5 \mid 6 \mid 7 \mid 8 \mid 9 \mid 10 \mid 11 \mid 12 \mid 13 \mid 14 \mid 15 \mid 16 \mid 17 \mid 18 \mid 19 \mid 20 \mid 21 \mid 22 \mid 23 \mid 24 \mid 25 \mid 26 \mid 27 \mid 28 \mid 29 \mid 30 \mid 31]

\textbf{mes} = [Enero \mid Febrero \mid Marzo \mid Abril \mid Mayo \mid Junio \mid Julio \mid Agosto \mid Septiembre \mid Octubre \mid Noviembre \mid Diciembre]

\textbf{nota} = [0 \mid 1 \mid 2 \mid 3 \mid 4 \mid 5 \mid 6 \mid 7 \mid 8 \mid 9 \mid 10]

\textbf{año} = 2 + dígito + dígito + dígito

\textbf{estado\_proyecto} = [sin\_evaluar \mid expertos\_evaluando \mid evaluado\_por\_expertos \mid evaluado\_por\_adjunto \mid validado\_coodinador]

\textbf{estado\_informe} = [en\_proceso \mid finalizado]

\textbf{asunto} = [alta\_usuario \mid invitación \mid aviso\_plazos]

\subsubsection{Flujos de datos}

\textbf{Aceptación} = *\emph{En el perfil del adjunto aparecerá la lista de expertos seleccionados y se indica si han aceptado o declinado}*

\textbf{Actualización Estado} = [evaluado\_por\_expertos \mid evaluado\_por\_adjunto \mid validado\_coodinador]

\textbf{Adjuntos} = [Consulta Subárea \mid Asignar Subárea]

\textbf{Adjuntos} = *\emph{Los usuarios adjuntos añadidos se asignan a una subárea. Se escribe en la tabla SUBÁREAS}*
 
\textbf{Alta Usuario} = id\_usuario + tipo + e-mail + contraseña

\textbf{Alta Usuario} = *\emph{El secretario da de alta a los usuarios introduciendo su identificador, el tipo de usuario, el e-mail y la contraseña}*

\textbf{Área} = id\_área + nombre

\textbf{Área} = *\emph{Los usuarios añadidos se asignan a una área}*

\textbf{Área Asignada} = id\_área

\textbf{Área Asignada} = *\emph{Se le añade un área a cada proyecto}*

\textbf{Área Proyectos} = id\_área + id\_paqproy + id\_proyecto

\textbf{Área Proyectos} = *\emph{Asignación de un área a un proyecto}*

\textbf{Asignar Subárea} = id\_subárea + id\_área + id\_usuario + nombre\_subárea.

\textbf{Asignar Subárea} = *\emph{Se introduce una fila nueva en la tabla SUBÁREAS}*

\textbf{Autentificación} = [Datos Autentificación \mid Verificar Autentificar]

\textbf{Autentificación} = *\emph{Acceso al sistema con una cuenta de usuario y verificación de la autentificación}*

\textbf{Avisos Plazos} = nombre + apellidos + [tpo\_inv \mid tpo\_eval] + id\_proyecto + id\_paquete + nombre\_proyecto + cuerpo

\textbf{Avisos Plazos} = *\emph{Correo de advertencia de fecha límite de evaluación de un proyecto}*

\textbf{Cambio Estado} = estado\_proyecto

\textbf{Cambio Estado} = *\emph{El estado del proyecto cambia de 'sin\_evaluar' a 'expertos\_evaluando'}*

\textbf{Consulta Área Proyecto} = área

\textbf{Consulta Área Proyecto} = *\emph{Se extrae el área del proyecto para buscar expertos relacionados con ese área}*

\textbf{Consulta Autentificación} = e-mail + contraseña

\textbf{Consulta Expertos} = [Expertos Seleccionados \mid Expertos Búsqueda \mid email]

\textbf{Consulta Informes Expertos} = id\_informe + id\_modelo\_eval + estado\_informe + datos

\textbf{Consulta Informes Expertos} = *\emph{Ver informes de proyectos de expertos}*

\textbf{Consulta Subárea} = id\_subárea + id\_área + (id\_usuario) + nombre

\textbf{Consulta Subárea} = *\emph{Consulta de las subáreas donde trabaja el usuario, el id\_usuario es opcional ya que el flujo consultar usuario que va desde modificar datos personales a Subáreas no utiliza el id\_usuario, sino que lee para asignarle un subárea}*

\textbf{Consulta Usuarios} = id\_usuario + tipo + e-mail + contraseña + nombre + apellidos, teléfono + id\_área + institución + currículum + palabras\_clave

\textbf{Convocatoria} = convocatoria

\textbf{convocatoria}: *\emph{En la convocatoria del paquete de proyectos encontramos todo lo necerario para realizar los Modelos de Evaluación}*

\textbf{Correo Alta} = id\_usuario + contraseña + nombre + apellidos + área + cuerpo

\textbf{Correo Alta} = *\emph{Correo de confirmación de registro de un nuevo usuario en el sistema}*

\textbf{Criterios Evaluación} = [id\_modelo + estructura \mid ID Modelo]

\textbf{Criterios Evaluación} = *\emph{Añadir, modificar o eliminar los modelos de evaluación a utilizar para realizar los informes}*

\textbf{Datos Autentificación} = id\_usuario + contraseña

\textbf{Datos Paquetes Proyectos} = (id\_paquete) + (institución) + (bases) + (convocatoria) + (fecha\_entrada) + (fecha\_salida) + (fecha\_límite)

\textbf{Datos Paquetes Proyectos} = *\emph{Se añaden, se modifican y se consultan los datos de los paquetes de proyectos}*

\textbf{Datos Personales} = id\_usuario + tipo + e-mail + contraseña + nombre + apellidos + teléfono + área + institución + currículum + palabras\_clave

\textbf{Datos Personales} = *\emph{Modificación de los datos personales de un usuario}*

\textbf{Datos Proyectos} = (id\_proyecto) + (id\_paquete) + (memoria) + (área) + (subárea) + (estado\_proyecto) + (tpo\_inv) + (tpo\_eval) + (id\_modelo\_eval) + (evaluación)

\textbf{Datos Proyectos} = *\emph{Se añaden, se modifican y se consultan los datos de los proyectos. Son todos opcionales por el flujo de datos en la modificación}*

\textbf{Datos Usuarios} = id\_usuario
\textbf{Datos Usuarios} = *\emph{Se le pasa el id del usuario para eliminarlo de la base de datos}*

\textbf{email} = email

\textbf{email} = *\emph{Se extrae el email de los expertos para mandarles la invitación}*

\textbf{Evaluaciones} = id\_paquete + {evaluación}

\textbf{Evaluaciones} = *\emph{Evaluaciones correspondientes a los proyectos de un paquete y que son enviadas a la institución convocante una vez que estén realizadas}*

\textbf{Experto Desasignado} = id\_usuario + id\_proyecto + id\_paquete

\textbf{Expertos} = [Parámetros Consulta \mid Resultado Consulta \mid Parámetros Búsqueda \mid Resultado Búsqueda \mid ID Experto \mid Experto Desasignado]

\textbf{Expertos} = *\emph{Tráfico relacionado con la búsqueda, consulta, asignación y desasignación de expertos para las evaluaciones de proyectos}*

\textbf{Expertos Búsqueda} = {@id\_usuario + tipo + e-mail + contraseña + nombre + apellidos + teléfono + área + institución + currículum + {palabras\_clave}}

\textbf{Expertos Búsqueda} = *\emph{Lista de expertos resultado de la consulta}*

\textbf{Expertos Seleccionados} = {@id\_usuario + tipo + e-mail + contraseña + nombre + apellidos + teléfono + área + institución + currículum + {palabras\_clave}}

\textbf{Expertos Seleccionados} = *\emph{Lista de expertos resultado de la consulta}*

\textbf{Expertos-Proyectos} = [Relación Expertos-Proyectos \mid Valoración Expertos \mid Fin Relación \mid Fecha Asignación \mid Nueva Relación \mid Valoración Trabajo]

\textbf{Fecha Asignación} = fecha\_asignación

\textbf{Fecha Asignación} = *\emph{Se utiliza para el aviso de plazos}*

\textbf{Fecha límite} = fecha\_límite

\textbf{Fecha límite} = *\emph{Se utiliza para indicar el plazo de realización del informe al experto}*

\textbf{Fin Relación} = id\_usuario + id\_proyecto + id\_paquete

\textbf{Fin Relación} = *\emph{Se elimina una tupla de la tabla EXPERTOS-PROYECTOS}*

\textbf{ID Experto} = id\_usuario

\textbf{ID Modelo} = id\_modelo

\textbf{Info Informe Evaluación} = [Informe Rechazado \mid Nuevo Informe]

\textbf{Info Paquetes Proyectos} = [Institución Convocante \mid Fecha límite \mid Plazo invitación]

\textbf{Información Proyecto} = [Tiempos \mid Consulta Área Proyecto \mid Cambio Estado \mid Nombre Proyecto]

\textbf{Informe Final} = id\_proyecto + id\_paquete + estado\_proyecto + evaluación

\textbf{Informe Final} = *\emph{Se muestran los datos referidos a los informes finales de los proyectos y también el coordinador introduce el informe final de evaluación del proyecto una vez finalizado este para ser devuelto a la institución convocante}*

\textbf{Informe Rechazado} = id\_informe

\textbf{Informe Rechazado} = *\emph{Se elimina la tupla correspondiente en la tabla INFORMES EVALUACIÓN}*

\textbf{Informes Adjuntos} = id\_informe + id\_modelo\_eval + estado\_informe + datos

\textbf{Informes Adjuntos} = *\emph{Ve y/o modifica el informe final de evaluación de un adjunto}*

\textbf{Informes Coordinadores} = id\_informe + id\_modelo\_eval + estado\_informe + datos

\textbf{Informes Coordinadores} = *\emph{Validar o modificar informe final del adjunto}*

\textbf{Informes Evaluación} = id\_informe + id\_modelo\_eval + estado\_informe + datos

\textbf{Informes Evaluación} = *\emph{Introducir, mostrar, modificar o eliminar los datos referidos a un informe de evaluación. Además modifica, cuando es oportuno, el estado en el que se encuentra dicho informe}*

\textbf{Informes Expertos} = id\_informe + id\_modelo\_eval + estado\_informe + datos

\textbf{Informes Expertos} = *\emph{Añadir o modificar informes de proyectos por parte de los expertos}*

\textbf{Institución Convocante} = institución

\textbf{Institución Convocante} = *\emph{Se consulta la institución convocante para que no coincida en la búsqueda de expertos}*

\textbf{Invitación} = [Petición \mid Aceptación]

\textbf{Invitación} = *\emph{Correo de invitación a un experto para que evalúe un proyecto y respuesta de éste}*

\textbf{Lista Usuarios} = {id\_usuario + tipo + e-mail + contraseña + nombre + apellidos + teléfono + área + institución + currículum + {palabras\_clave}}

\textbf{Lista Usuarios} = *\emph{Lista devuelta por una búsqueda o consulta de usuarios}*

\textbf{Memoria Proyecto} = id\_proyecto + id\_paquete + memoria + id\_modelo\_eval + estado\_proyecto

\textbf{Memoria Proyecto} = *\emph{Se lee para poder realizar el informe. El adjunto también leerá el estado en que se encuentra la evaluación del proyecto}*

\textbf{Modelo} = id\_modelo\_eval

\textbf{Modelo} = *\emph{Se consulta el id del modelo de evaluación para poder asignárselo a los proyectos y también se consulta por si se quiere hacer un cambio en paquetes de proyectos}*

\textbf{Modelos Evaluación} = [id\_modelo + estructura \mid Nuevo Modelo Evaluación \mid id\_modelo]

\textbf{Modelos Evaluación} = *\emph{Insertar, modificar o eliminar las estructuras que deben seguir los informes de evaluación de cada proyecto}*

\textbf{Nombre Proyecto} = id\_proyecto + nombre

\textbf{Nueva Relación} = id\_usuario + id\_proyecto + id\_paquete + id\_informe + valoración + fecha\_asignación

\textbf{Nueva Relación} = *\emph{Se inserta una tupla en la tabla EXPERTOS-PROYECTOS}*

\textbf{Nuevo Informe} = id\_informe + id\_modelo\_eval + estado\_informe + datos

\textbf{Nuevo Informe} = *\emph{Se inserta una tupla en la tabla INFORMES EVALUACIÓN. El estado\_informe será 'en\_proceso'}*

\textbf{Nuevo Modelo Evaluación} = id\_modelo + estructura

\textbf{Nuevo Modelo Evaluación} = *\emph{Añade un nuevo Modelo de Evaluación}*

\textbf{Paquetes Proyectos} = [id\_paquete + institución + nº\_de\_proyectos + bases + convocatoria + fecha\_entrada + fecha\_salida + fecha\_límite \mid Proyectos]

\textbf{Paquetes Proyectos} = *\emph{Paquetes de proyectos enviados por la institución convocante para su evaluación}*

\textbf{Parámetros} = (id\_usuario) + (tipo) + (e-mail) + (nombre) + (apellidos) + (teléfono) + (área) + (institución) + (currículum) + (palabras\_clave)

\textbf{Parámetros} = *\emph{Son los parámetros de búsqueda de usuarios. Todos los campos con opcionales ya que se pueden realizar búsquedas de usuarios de varias maneras}*

\textbf{Parámetros Consulta} = Parámetros Búsqueda = (@id\_usuario) + (tipo) + (e-mail) + (contraseña) + (nombre) + (apellidos) + (teléfono) + (área) + (institución) + (currículum) + (palabras\_clave)

\textbf{Petición} = id\_usuario + nombre + apellidos + id\_proyecto + nombre\_proyecto + tmp\_invitación + Correo\_invitación

\textbf{Plantilla} = id\_plantilla + asunto + cuerpo.

\textbf{Plantilla} = *\emph{Para mandar los correos pertinentes}*

\textbf{Plazo Invitación} = tmp\_invitación

\textbf{Proyecto Rechazado} = id\_proyecto + id\_paquete + id\_subárea

\textbf{Proyecto Rechazado} = *\emph{Aviso del adjunto al coordinador de su área para informar de que el proyecto no corresponde a su subárea y debe, por tanto, asignarse a otro subárea distinta}*

\textbf{Proyectos} = id\_proyecto + id\_paquete + nombre\_proyecto + memoria + área + subárea + estado\_proyecto + tpo\_inv + tpo\_eval + id\_modelo\_eval + evaluación

\textbf{Proyectos} = *\emph{Consulta la memoria del proyecto para realizar el informe de evaluación}*

\textbf{Relación Expertos-Proyectos} = {id\_usuario + id\_proyecto + id\_paquete + id\_informe + valoración + fecha\_asignación}

\textbf{Relación Expertos-Proyectos} = *\emph{Lista de la relación entre Expertos y Proyectos. Se devolverá una lista según los parámetros de consulta}*

\textbf{Resultado Consulta} = Resultado Búsqueda = {@id\_usuario + tipo + e-mail + contraseña + nombre + apellidos + teléfono + área + institución + currículum + {palabras\_clave}}

\textbf{Subárea Adjuntos} = id\_subárea + id\_usuario

\textbf{Subárea Adjuntos} = *\emph{Asignación de un adjunto a una subárea}*

\textbf{Subárea Asignada} =id\_subárea

\textbf{Subárea Asignada} = *\emph{Se le añade un subárea a cada proyecto}*

\textbf{Subárea Proyectos} = id\_subárea + id\_proyecto + id\_paquete

\textbf{Subárea Proyectos} = *\emph{Asignación de un proyecto a una subárea}*

\textbf{Tiempos} = tpo\_eval

\textbf{Tiempos} = *\emph{Se consulta el tiempo de evaluación para saber cuanto le queda al experto para realizar el informe}*

\textbf{Usuarios} = [id\_usuario + tipo + e-mail + contraseña + (nombre) + (apellidos) + (teléfono) + (área) + (institución) + (currículum) + (palabras\_clave) \mid Parámetros]

\textbf{Usuarios} = *\emph{Da el alta a los usuarios, los consulta o modifica sus datos. Modifica el coordinador asociado a un área. Incluye los parámetros de búsqueda introducidos para realizar dicha búsqueda de usuarios}*

\textbf{Valoración Expertos} = id\_usuario + id\_proyecto + id\_paquete + tpo\_realización + calidad\_informe

\textbf{Valoración Expertos} = *\emph{Cada informe elaborado por un experto para un proyecto, recibirá una valoración según el tiempo de realización y la calidad del informe}*

\textbf{Valoración trabajo} = {tpo\_realización + calidad\_informe}

\textbf{Valoración trabajo} = *\emph{Se utiliza en búsqueda de expertos}*

\textbf{Verificar Autentificar} = *\emph{Mostrar que la autentificación es correcta. Si introduce incorréctamente el id o la contraseña o ambas, se mostrará una plantalla en la que se indicará: 'Usuario o contraseña son incorrectas'}*
